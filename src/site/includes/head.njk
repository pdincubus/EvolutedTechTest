<meta charset="UTF-8">
<title>{{ title | safe }} - {{ settings.sitename }}</title>
<meta name="viewport" content="width=device-width">

<script async>
    document.documentElement.classList.remove('no-js');
    document.documentElement.classList.add('js');
</script>

<script nomodule>
    document.documentElement.classList.add('is-old-browser');
</script>

<link rel="stylesheet" href="{{ '/css/site.css' | url }}">
<link rel="preload" href="{{ '/css/site.css' | url }}" as="style">
<link rel="preload" href="{{ '/js/index.js' | url }}" as="script">

<link rel="preload" href="{{ '/fonts/lato-400.woff2' | url }}" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="{{ '/fonts/lato-700.woff2' | url }}" as="font" type="font/woff2" crossorigin>

<script type="module" src="{{ '/js/index.js' | url }}"></script>